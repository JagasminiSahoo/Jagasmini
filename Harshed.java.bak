import java.util.Scanner;
class Harshed 
{
	public static void main(String[] args) 
	{
	  Scanner sc=new Scanner(System.in);
	  System.out.println("Enter a number:");
	  int n=sc.nextInt();
	  int sum=0;
	  int rem=0;
	  int num=n;
	  while(n>0)
		{
		   rem=n%10;
	       sum=sum+rem;
		   n=n/10;
	   }
	   if(num%sum==0)
		{
			System.out.println("Harshed number");
		}
       else
		{
			System.out.println("Not a Harshed number");
		}
}
}
		